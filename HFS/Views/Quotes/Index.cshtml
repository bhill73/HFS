@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Symbol)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Q_Last)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Q_Change)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Q_52Wk_High)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Q_52Wk_Low)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PEG)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PE)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
