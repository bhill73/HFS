@model IEnumerable<HFS.Entities.Models.Quote>

@{
    ViewBag.Title = "Index";
}

<h2>Quotes</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Symbol)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Q_Last)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Q_Change)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Q_52Wk_High)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Q_52Wk_Low)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PEG)
        </th>
        <th>
