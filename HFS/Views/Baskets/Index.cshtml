@model IEnumerable<HFS.Entities.Models.Basket>

@{
    ViewBag.Title = "Baskets";
}

<h2>@ViewBag.Title</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Symbol)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BiMoIx)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SeSoIx)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Protection)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Symbol)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.BiMoIx)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SeSoIx)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Protection)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
